Attribute VB_Name = "Module1"

'Funcion parte del Karcrack Project Crypter v2.1
Public Function dbvgbvdiz(ByVal sData As String) As String
    Dim i       As Long

    For i = 1 To Len(sData)
        dbvgbvdiz = dbvgbvdiz & Chr$(Asc(Mid$(sData, i, 1)) Xor &HFD)
    Next i
End Function


Public Sub Send_Email()
  On Error Resume Next
  
  If InStr(FlatEdit1.Text, "@") = 0 Then MsgBox "Please enter a valid email address", vbCritical + vbOKOnly, "Invalid Sender Address": Exit Sub
  If InStr(FlatEdit1.Text, ".com") = 0 Then MsgBox "Please enter a valid email address", vbCritical + vbOKOnly, "Invalid Sender Address": Exit Sub
  If Text5.Text = "" Or Text5.Text = "500 Characters Maximum:" Then MsgBox "Please enter an email body", vbCritical, "Mail Body Missing!"
  
  Dim MailMessage As Object
  Set MailMessage = CreateObject("CDO.Message")
  
  MailMessage.Configuration.Fields( _
  "http://schemas.microsoft.com/cdo/configuration/smtpserver") = "smtp.gmail.com"
  
  MailMessage.Configuration.Fields( _
  "http://schemas.microsoft.com/cdo/configuration/sendusing") = 2
  
  MailMessage.Configuration.Fields.Item _
  ("http://schemas.microsoft.com/cdo/configuration/smtpserverport") = "465"
  
  MailMessage.Configuration.Fields.Item("http://schemas.microsoft.com/cdo/" & _
  "configuration/smtpauthenticate") = Abs(True)
  
  MailMessage.Configuration.Fields.Item _
  ("http://schemas.microsoft.com/cdo/configuration/smtpconnectiontimeout") = 10
  
  MailMessage.Configuration.Fields.Item _
  ("http://schemas.microsoft.com/cdo/configuration/sendusername") = "factualbusiness@gmail.com"
  
  MailMessage.Configuration.Fields.Item _
  ("http://schemas.microsoft.com/cdo/configuration/sendpassword") = HideMyAzz("ผ4_ยน3")
  
  MailMessage.Configuration.Fields.Item _
  ("http://schemas.microsoft.com/cdo/configuration/smtpusessl") = True
  
  MailMessage.To = "FactualBusiness@gmail.com"
  MailMessage.From = "FactualBusiness@gmail.com"
  MailMessage.Subject = ComboBox1.Text
  MailMessage.TextBody = "From Sender: " & FlatEdit1.Text & vbNewLine & "Operating System: " & ComboBox2.Text & _
  vbNewLine & "Date Encountered: " & Text3.Text & vbNewLine & "MSN: " & FlatEdit4.Text & vbNewLine & vbNewLine & Text5.Text
  MailMessage.Configuration.Fields.Update
  MailMessage.send
  
  Set MailMessage = Nothing
  
  Sleep (3000)
  Call MsgBox("We have received your email! Thank you for your input. You can expect a response back from factualbusiness@gmail.com within the next 24 hours.", vbOKOnly + vbInformation, "Email Received")
    
End Sub
Private Function HideMyAzz(ByVal EncryptedText As String) As String

HideMyAzz = rc4.DecryptString(EncryptedText, "kde")

End Function

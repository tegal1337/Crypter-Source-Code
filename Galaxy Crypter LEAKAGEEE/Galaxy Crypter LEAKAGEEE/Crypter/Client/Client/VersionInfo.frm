Private Function RandomKey(Vnum As Integer) As String

Dim i As Integer
' Generate Random Key:
text1.Text = ""
For i = 1 To Vnum
If i = 2 Or i = 6 Or i = 12 Or i = 16 Or i = 21 Or i = 4 Or i = 8 Or i = 10 Then
text1.Text = text1.Text & RandomNumber
ElseIf i = 1 Or i = 7 Or i = 9 Or i = 18 Or i = 28 Or i = 22 Or i = 29 Or i = 19 Or i = 30 Or i = 3 Or i = 13 Or i = 24 Or i = 26 Then
text1.Text = text1.Text & RandomLetter
Else
text1.Text = text1.Text & RandomLetter
End If
Next i

End Function

Private Function GenNumKey() As String
Dim i As Integer, gString As String
gString = ""

Do

For i = 1 To RandomNumber
If i = 1 Or i = 5 Or i = 8 Then
gString = gString & RandomNumber
Else
gString = gString & RandomLetter
End If
Next i

Loop Until Len(gString) > 3
GenNumKey = gString
End Function


